<MainWindow>:
    name: "title"

    FloatLayout:
        Video:
            source:"Logo.mp4"
            state:"play"
            options: {'eos': 'loop'}
        Button:
            text: "Next"
            font_size: 15
            size_hint: 0.10, 0.1
            background_normal: 'next.png'
            background_down: 'next.png'
            pos_hint: {"x":0.47, "y":0.15}
            on_release:
                app.root.current = "m"
                root.manager.transition.direction = "left"


<FirstWindow>:
    name: "first"

    FloatLayout:
        canvas.before:
            Color:
                rgba: (1, 1, 1, 1)
            Rectangle:
                source:'background_image.jpg'
                size: root.width, root.height
                pos: self.pos
        Button:
            text: "USER"
            font_size: 40
            size_hint: 0.45, 0.3
            background_color: 0, 128, 0, 0.5
            pos_hint: {"x":0.30, "y":0.17}
            on_release:
